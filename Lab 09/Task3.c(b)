// Online C compiler to run C program online
#include <stdio.h>
void GCD(int num1,int num2){
    
}
int main() {
    int num1,num2,gcd=0;
    printf("Enter num1 and num2:");
    scanf("%d %d", &num1,&num2);
    // Finding GCD by Using loop
    for(int i=num1/2;i>1;i--){
        if(num1%i==0){
            if(num2%i==0){
                gcd=i;
                break;
            }
        }
    }
    printf("GCD by using Loop:%d", gcd);
    GCD(num1,num2);
    return 0;
}
